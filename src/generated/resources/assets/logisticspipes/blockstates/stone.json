{
  "multipart": [
    {
      "apply": {
        "model": "logisticspipes:block/center"
      }
    },
    {
      "apply": {
        "model": "logisticspipes:block/end"
      },
      "when": {
        "connection_north": "true"
      }
    },
    {
      "apply": {
        "model": "logisticspipes:block/end",
        "y": 180
      },
      "when": {
        "connection_south": "true"
      }
    },
    {
      "apply": {
        "model": "logisticspipes:block/end",
        "y": 90
      },
      "when": {
        "connection_east": "true"
      }
    },
    {
      "apply": {
        "model": "logisticspipes:block/end",
        "y": 270
      },
      "when": {
        "connection_west": "true"
      }
    },
    {
      "apply": {
        "model": "logisticspipes:block/end",
        "x": 90
      },
      "when": {
        "connection_down": "true"
      }
    },
    {
      "apply": {
        "model": "logisticspipes:block/end",
        "x": 270
      },
      "when": {
        "connection_up": "true"
      }
    }
  ]
}